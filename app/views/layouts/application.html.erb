<!DOCTYPE html>
<html>
<head>
  <title>PasswordApp</title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag :defaults, "rails.validations" %>
  <%= csrf_meta_tag %>
  <link rel="shortcut icon" href="/images/favicon.ico" type="image/ico" />
</head>
<body>

<div id="container">
  <div id="outerHeader">
    <%= render "shared/header" %>
  </div>
  <div id="content">
  <%= yield %>
  </div>
  <%= render "shared/footer" %>
</div>

</body>
</html>
